""" Move to Mirror folder """
try:
    shutil.copytree(RA_Mirror + Current_folder, RA_Archive + Day)
    shutil.rmtree(RA_Mirror + Current_folder)
    shutil.copytree(RA_src, RA_Mirror + Current_folder)
except OSError:
    arcpy.AddMessage("FAIL.. creation of RA Mirror Directory")
